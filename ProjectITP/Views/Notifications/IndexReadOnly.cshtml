@model IEnumerable<ProjectITP.Models.Notification>

@{
    ViewBag.Title = "Index";
   
}

<h2 style="font-family:fantasy">Student Notifications</h2>

<br >
<br >
<table class="table table-striped table-bordered">
   

@foreach (var item in Model) {
    <tr style="font-style:oblique ">
        <td>
            <h4 style="color:blue"> @Html.DisplayFor(modelItem => item.Title)</h4>
        </td>

    </tr>
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        
    </tr>
}

</table>
